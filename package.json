{
  "name": "iot",
  "private": true,
  "version": "1.0.0",
  "packageManager": "yarn@3.2.1",
  "repository": "git@github.com:mmatloch/iot.git",
  "author": "Mateusz Matloch",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "script": "zx ./packages/scripts/src/index.mjs"
  },
  "importSort": {
    ".js, .ts, .tsx, .mjs": {
      "parser": "typescript",
      "style": "custom",
      "options": {
        "cacheStrategy": "directory",
        "wildcardAtStart": false,
        "extraAliases": [],
        "ignoredAliases": [],
        "bottomAliases": [],
        "ignoreTsConfig": "false",
        "spaceAfterAliases": true
      }
    }
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-custom": "^2.1.2",
    "prettier": "^2.6.2",
    "prettier-plugin-sorted": "^2.0.0",
    "typescript": "^4.7.3",
    "zx": "^7.0.7"
  }
}
